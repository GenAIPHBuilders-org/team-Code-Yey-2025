{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "frontend:development": "next dev --turbopack",
    "frontend:build": "next build",
    "backend:development": "cd ../backend && python -m venv venv && source venv/bin/activate && pip install -r requirements.txt && uvicorn app:app --reload",
    "start": "concurrently --kill-others \"npm run frontend:development\" \"npm run backend:development\"",
    "lint": "next lint"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "next": "15.1.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
